<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ebe3cd"
    tools:context=".DetailsStationsActivity">

    <TextView
        android:id="@+id/operationnel"
        android:layout_width="30dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toTopOf="@+id/capacity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/operationnel_tv"
        app:layout_constraintTop_toBottomOf="@+id/station_name" />

    <TextView
        android:id="@+id/operationnel_tv"
        android:layout_width="105dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toTopOf="@+id/capacity_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/station_name" />

    <Button
        android:id="@+id/add_to_favorites_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:iconTint="#80D8FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nb_ebike" />

    <TextView
        android:id="@+id/nb_ebike"
        android:layout_width="30dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.005"
        app:layout_constraintStart_toEndOf="@+id/nb_ebike_tv"
        app:layout_constraintTop_toBottomOf="@+id/nb_mech"
        app:layout_constraintVertical_bias="0.232" />

    <TextView
        android:id="@+id/nb_ebike_tv"
        android:layout_width="195dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nb_mech_tv"
        app:layout_constraintVertical_bias="0.232" />

    <TextView
        android:id="@+id/nb_mech"
        android:layout_width="30dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/nb_mech_tv"
        app:layout_constraintTop_toBottomOf="@+id/nb_borne_dispo"
        app:layout_constraintVertical_bias="0.19"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nb_mech_tv"
        android:layout_width="200dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nb_borne_dispo_tv"
        app:layout_constraintVertical_bias="0.19" />

    <TextView
        android:id="@+id/nb_borne_dispo"
        android:layout_width="30dp"
        android:layout_height="18dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/nb_borne_dispo_tv"
        app:layout_constraintTop_toBottomOf="@+id/nb_velo_dispo"
        app:layout_constraintVertical_bias="0.154" />

    <TextView
        android:id="@+id/nb_borne_dispo_tv"
        android:layout_width="200dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nb_velo_dispo_tv"
        app:layout_constraintVertical_bias="0.154" />

    <TextView
        android:id="@+id/nb_velo_dispo"
        android:layout_width="30dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/nb_velo_dispo_tv"
        app:layout_constraintTop_toBottomOf="@+id/capacity"
        app:layout_constraintVertical_bias="0.13" />

    <TextView
        android:id="@+id/nb_velo_dispo_tv"
        android:layout_width="200dp"
        android:layout_height="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/capacity_tv"
        app:layout_constraintVertical_bias="0.131" />

    <TextView
        android:id="@+id/station_name"
        android:layout_width="334dp"
        android:layout_height="30dp"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/capacity_tv"
        android:layout_width="65dp"
        android:layout_height="18dp"
        android:layout_marginTop="48dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/station_name"
        app:layout_constraintVertical_bias="0.156"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/capacity"
        android:layout_width="30dp"
        android:layout_height="18dp"
        android:layout_marginTop="46dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/nb_velo_dispo_tv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/capacity_tv"
        app:layout_constraintTop_toBottomOf="@+id/station_name"
        app:layout_constraintVertical_bias="0.6"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>

